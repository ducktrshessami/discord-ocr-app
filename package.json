    "discord-command-options": "ducktrshessami/discord-command-options#v0.2.0",
    "tesseract.js": "^7.0.0",
    "undici": "^7.22.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "zlib-sync": "^0.1.10"
